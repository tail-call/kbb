---Description of a class
---@class ${name}
---@field __module '${name}'
---# Properties
---@field demoProp any Any property
---# Methods
---@field setDemoProp fun(self: ${name}, newValue: any)

local M = require 'core.class'.define{..., metatable = {
  ---@type ${name}
  __index = {
    setDemoProp = function (self, newValue)
      self.demoProp = newValue
    end,
  }
}}

---@param obj ${name}
function M.init(obj)
  require 'core.Dep' (obj, function (want)
    return { want.demoProp }
  end)
end

return M